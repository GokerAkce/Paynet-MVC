<!DOCTYPE html>
<html lang="tr" class="no-js">
<head>
	<meta http-equiv="Content-Type" content="text/html; charset=windows-1254">
	<meta name="robots" content="noindex">
	<meta name="googlebot" content="noindex">
	<meta name="googlebot-news" content="noindex">
	<meta name="googlebot-news" content="nosnippet">
	<meta name="viewport" content="initial-scale=1.0 maximum-scale=1.0 user-scalable=no">
	<title>Paynet | @ViewBag.Company </title>
	<meta http-equiv="Cache-Control" content="no-cache">
	<meta http-equiv="Pragma" content="no-cache">
	<meta http-equiv="Expires" content="-1">
	<link href="~/Content/style.css" rel="stylesheet">
	<link href="~/Content/bootstrap.min.css" rel="stylesheet" type="text/css">
	

	<!-- HTML5 shim and Respond.js for IE8 support of HTML5 elements and media queries -->
	<!-- WARNING: Respond.js doesn't work if you view the page via file:// -->
	<!--[if lt IE 9]>
	  <script src="https://oss.maxcdn.com/html5shiv/3.7.3/html5shiv.min.js"></script>
	  <script src="https://oss.maxcdn.com/respond/1.4.2/respond.min.js"></script>
	<![endif]-->

</head>
<body>
	<div class="container max700 mtop50">
		<div class="tab-content">
			<div class="tab-pane fade in active" id="tab1">
				<div class="panel panel-default">
					<div class="panel-body">
						<div class="row txtpd">
							<a href="javascript:void(0)" target="_blank"><img src="@ViewBag.LogoUrl" alt=""></a>
							<br>
						</div>
						<div class="row">
							<div class="col-md-12">
								<div class="well well-sm">Açıklama: <b>@ViewBag.Desc</b></div>
								<div class="well well-sm">Ödenecek Tutar: <b>@ViewBag.Amount TL.</b></div>
							</div>
							<div class="col-md-12">
								<form action="@Url.Action("chargetransaction","paynet")" method="post">
									<script class="paynet-button" type="text/javascript" src="https://pj.paynet.com.tr/public/js/paynet.min.js" data-key="@ViewBag.PublicKey" data-amount="@(ViewBag.DataAmount)" data-ratio_code="" data-no_instalment="@ViewBag.Instalment" data-agent="@ViewBag.AgentCode" data-add_commission_amount="@ViewBag.Commission" data-tds_required="@ViewBag.TDS" data-description="@ViewBag.Desc"  data-button_label="Ödeme Yap" data-image="@ViewBag.LogoUrl">
									</script>
									<input type="hidden" name="customerCode" value="@ViewBag.CustomerCode" />
								</form>
							</div>
						</div>
					</div>
				</div>
			</div>
		</div>
	</div>
	<div class="iletisims" style="position:fixed;bottom:0px;margin-bottom:0px;    background-color: #f9f9f9;
    width: 100%;
    padding: 5px 0px;font-size:10px;text-align:center;">
		@ViewBag.Company Bayi Ödeme Sistemi <a href="mailto:@(ViewBag.Mail)">@ViewBag.Mail</a> | @ViewBag.Phone | @ViewBag.Address
	</div>
</body>
</html>
